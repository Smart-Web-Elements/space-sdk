{
  "name": "swe/space-sdk",
  "description": "A PHP package to interact with JetBrains Space.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "jetbrains",
    "phpstorm",
    "space",
    "sdk",
    "api",
    "php"
  ],
  "authors": [
    {
      "name": "Luca Braun",
      "email": "l.braun@s-w-e.com"
    }
  ],
  "homepage": "https://www.smart-web-elements.com",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "vlucas/phpdotenv": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Swe\\SpaceSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Swe\\SpaceSDK\\Tests\\": "tests/"
    }
  },
  "support": {
    "email": "info@s-w-e.com"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "readme": "readme.md"
}
